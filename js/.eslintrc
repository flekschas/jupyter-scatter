{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "require": false,
    "module": false,
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "no-plusplus": 0
  }
}
